{
  "name": "miwago-api-development",
  "private": true,
  "version": "0.0.1",
  "description": "backend",
  "scripts": {
    "start-once": "cross-env NODE_CONFIG_DIR=./src/config ts-node ./src/bin/www.ts",
    "start": "cross-env NODE_CONFIG_DIR=./src/config cross-env TESTING=true nodemon --config ./nodemon.json --exec ts-node ./src/bin/www.ts",
    "start-dist": "cd dist/src/public && yarn install && cd ../../.. && cross-env NODE_CONFIG_DIR=./dist/src/config node ./dist/src/bin/www",
    "build": "gulp build",
    "build-with-docs": "gulp build-with-docs",
    "lint-noFix": "gulp lint-noFix",
    "lint": "npm run prettier && npm run tslint",
    "test": "npm run build && npm run reset-test-db && npm run start-test-system",
    "test-with-docs": "npm run build-with-docs && npm run reset-test-db && npm run start-test-system",
    "start-test-system": "TESTING=true node tests/start-test-system.js",
    "test-core": "cross-env LOG_LEVEL=error TESTING=true NODE_APP_INSTANCE=test NODE_CONFIG_DIR=./dist/src/config jest --config=./ci-jest.config.json --forceExit --detectOpenHandles --maxWorkers=2",
    "ts-test": "npm run reset-test-db && export TESTING=true LOG_LEVEL=error && NODE_CONFIG_DIR=./src/config NODE_APP_INSTANCE=test jest --forceExit --detectOpenHandles",
    "copy-assets": "gulp copy-assets && gulp replace",
    "clean-build": "gulp clean-build",
    "compile": "tsc",
    "prettier": "prettier '{src/**/*.*,typings/**/*.*,tests/**/*.*}' --write",
    "tslint": "tslint -c tslint.json -p tsconfig.json -t stylish --fix '{src,tests}/**/*.ts'",
    "reset-test-db": "NODE_CONFIG_DIR=./src/config node tests/reset.js",
    "devapi-miwago": "export NODE_ENV=production && export NODE_APP_INSTANCE=beta && node bin/www.js",
    "production-staging": "export NODE_ENV=production && export NODE_APP_INSTANCE=staging && node bin/www.js",
    "production-production": "export NODE_ENV=production && export NODE_APP_INSTANCE=production && node bin/www.js"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "aws-sdk": "^2.267.1",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "config": "^1.30.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cron": "^1.4.1",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-query-boolean": "^2.0.0",
    "express-query-int": "^3.0.0",
    "express-validator": "^5.2.0",
    "extract-zip": "^1.6.7",
    "fancy-log": "^1.3.2",
    "got": "^9.2.2",
    "fast-csv": "^2.4.1",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "graphql-type-json": "^0.2.1",
    "html-pdf": "^2.2.0",
    "issue-maker": "^0.0.11",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "left-pad": "^1.3.0",
    "lme": "^1.5.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "node-ipc": "^9.1.1",
    "ps-node": "^0.1.6",
    "rimraf": "^2.6.2",
    "serialize-error": "^2.1.0",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.8",
    "swagger-jsdoc": "^1.9.7",
    "through2": "^2.0.3",
    "url-pattern": "^1.0.3",
    "user-home": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/bluebird": "^3.5.21",
    "@types/body-parser": "^1.17.0",
    "@types/config": "^0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/cron": "^1.3.0",
    "@types/del": "^3.0.1",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/extract-zip": "1.6.2",
    "@types/fancy-log": "^1.3.0",
    "@types/got": "^8.3.3",
    "@types/graphql": "^0.13.3",
    "@types/graphql-type-json": "^0.1.2",
    "@types/gulp": "^4.0.5",
    "@types/gulp-rename": "^0.0.33",
    "@types/gulp-replace": "^0.0.31",
    "@types/html-minifier": "^3.5.2",
    "@types/html-pdf": "^2.1.2",
    "@types/jest": "^23.1.4",
    "@types/joi": "^13.3.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.116",
    "@types/mkdirp": "^0.5.2",
    "@types/moment-timezone": "^0.5.9",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "1.3.7",
    "@types/node": "^10.5.1",
    "@types/node-ipc": "^9.1.1",
    "@types/public-ip": "^2.3.0",
    "@types/rimraf": "2.0.2",
    "@types/run-sequence": "^0.0.30",
    "@types/serialize-error": "^2.1.0",
    "@types/serve-favicon": "^2.2.30",
    "@types/shortid": "^0.0.29",
    "@types/swagger-jsdoc": "^0.0.2",
    "@types/through2": "^2.0.34",
    "@types/user-home": "2.0.0",
    "del": "^3.0.0",
    "git-last-commit": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-exec": "^3.0.1",
    "gulp-markdown": "^2.0.1",
    "gulp-prettier-plugin": "^1.3.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.2",
    "html-minifier": "^3.5.18",
    "jest": "^23.2.0",
    "markdown-table": "^1.1.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "run-sequence": "^2.2.1",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.spec.{ts,js}"
    ],
    "testEnvironment": "node"
  }
}
